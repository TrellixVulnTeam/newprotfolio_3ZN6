m.length; i++ ){\r\n                    itemArr.push( item[i].parentNode );\r\n                }\r\n                setTimeout( function(){\r\n                    itemArr.forEach( function( index ){\r\n                        TweenMax.set( itemArr, { opacity:1 });\r\n                        TweenMax.to( itemArr, 0.1, { opacity:0, visibility:\"hidden\", ease:Power0.easeInOut, delay: index * 0.01 });\r\n                    })\r\n                },2000 )\r\n            },\r\n            textShow: function(){\r\n                let textArr = [];\r\n                let textTarget = document.querySelectorAll( \".title__list span\" );\r\n\r\n                textTarget.forEach( function( element, index ){\r\n                    TweenMax.set( element, { y: 50, opacity:0 });\r\n                    TweenMax.to( element, 0.35, { y: 0, opacity:1, ease:Power0.easeInOut, delay: index * 0.15 });\r\n                })\r\n            \r\n            },\r\n            handleScroll( vertical, horizontal, nativeEvent ){\r\n                let sT =  vertical.scrollTop / 100;\r\n                let items = document.querySelectorAll( \".gallery__item\" );\r\n                let items3 = this.scrollYpointer[2].y;\r\n\r\n                items.forEach( function( element, index ){\r\n                    var item1 = items[0];\r\n                    var item2 = items[1];\r\n                    var item3 = items[2];\r\n                    var item4 = items[3];\r\n                    var item5 = items[4];\r\n                    console.log(  items3, sT, \"=============>\", items3 + sT );\r\n                    TweenMax.to( item1, 0.75, { x: -(sT * 2) +\"vw\", ease:Power0.easeInOut });\r\n                    TweenMax.to( item2, 0.75, { x: -(sT * 5) +\"vw\", ease:Power0.easeInOut, delay: .1 });\r\n                    TweenMax.to( item3, 0.35, { y: (items3 + sT) + \"vw\", ease:Power0.easeInOut });\r\n                    TweenMax.to( item4, 0.75, { x: (sT * 3) +\"vw\", ease:Power0.easeInOut });\r\n   